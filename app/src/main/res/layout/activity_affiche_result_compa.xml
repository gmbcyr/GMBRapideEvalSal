                    style="@style/text_line_val"
                    android:background="@drawable/line1_bg"
                    android:gravity="center_vertical|center_horizontal"/>


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="gross Freq value"
                    android:id="@+id/federalTaxesVal_freq"
                    android:layout_column="0"
                    style="@style/text_line_val"
                    android:layout_below="@+id/federalTaxes"
                    android:layout_alignParentLeft="true"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Gross in year"
                    android:id="@+id/federalTaxesVal_total"
                    android:layout_column="5"

                    style="@style/text_line_val"
                    android:layout_below="@+id/federalTaxes"
                    android:layout_alignParentRight="true"/>

            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/lyt_tax_ssecu"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lyt_tax_federal">

