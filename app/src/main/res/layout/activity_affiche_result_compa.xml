                    android:background="@drawable/line1_bg"
                    android:gravity="center_vertical|center_horizontal"/>


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="$$$"
                    android:id="@+id/social_security_taxes_val_freq"
                    android:layout_column="0"
                    style="@style/text_line_val"
                    android:layout_below="@+id/social_security_taxes"
                    android:layout_alignParentLeft="true"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="$$$"
                    android:id="@+id/social_security_taxes_val_total"
                    android:layout_column="5"

                    style="@style/text_line_val"
                    android:layout_below="@+id/social_security_taxes"
                    android:layout_alignParentRight="true"/>



            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/lyt_tax_medicc"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lyt_tax_ssecu">

                <TextView
                    android:id="@+id/EmployeeMedic"
                    style="@style/text_line_val"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignEnd="@+id/employee_medicare_Val_total"
                    android:layout_alignParentTop="true"
                    android:layout_alignRight="@+id/employee_medicare_Val_total"
                    android:layout_column="0"
                    android:background="@drawable/line1_bg"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="@string/employe_medicare" />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="$$$"
                    android:id="@+id/employee_medicare_Val_freq"
                    android:layout_column="0"
                    style="@style/text_line_val"
                    android:layout_below="@+id/EmployeeMedic"
                    android:layout_alignParentLeft="true"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="$$$"
                    android:id="@+id/employee_medicare_Val_total"
                    android:layout_column="5"

                    style="@style/text_line_val"
                    android:layout_below="@+id/EmployeeMedic"
                    android:layout_alignParentRight="true"/>




            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/lyt_tax_other"
