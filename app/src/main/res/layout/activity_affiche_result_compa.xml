                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Frequency(weekly)"

                    android:id="@+id/earnings_freg"
                    android:layout_alignParentLeft="true"
                    style="@style/text_line_val"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:text="Total per Year"
                    android:id="@+id/earnings_total"
                    android:layout_alignParentRight="true"
                    style="@style/text_line_val"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/lyt_pay_stand"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lyt_earnings_title">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/regular_pay"
                    android:id="@+id/regular_pay_title"
                    android:layout_column="0"
                    style="@style/text_line_val"
                    android:background="@drawable/line1_bg"
                    android:gravity="center_vertical|center_horizontal"/>


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Freq value"
                    android:id="@+id/standard_pay_freg"
                    android:layout_column="0"
                    style="@style/text_line_val"
                    android:layout_below="@+id/regular_pay_title"
                    android:layout_alignParentLeft="true"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Gross in year"
                    android:id="@+id/standard_pay_total"
                    android:layout_column="5"

                    style="@style/text_line_val"
                    android:layout_below="@+id/regular_pay_title"
                    android:layout_alignParentRight="true"/>

            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/lyt_ot_pay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lyt_pay_stand">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/overtime_pay"
                    android:id="@+id/ot_pay_title"
                    android:layout_column="0"
                    style="@style/text_line_val"
                    android:background="@drawable/line1_bg"
                    android:gravity="center_vertical|center_horizontal"/>


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="$$$"
                    android:id="@+id/ot_pay_freq"
