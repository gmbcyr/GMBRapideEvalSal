                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/other_taxes"
                    android:id="@+id/othersTaxes"
                    android:layout_column="0"
                    style="@style/text_line_val"
                    android:background="@drawable/line1_bg"
                    android:gravity="center_vertical|center_horizontal"/>


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="$$$"
                    android:id="@+id/others_taxes_Val_freq"
                    android:layout_column="0"
                    style="@style/text_line_val"
                    android:layout_below="@+id/othersTaxes"
                    android:layout_alignParentLeft="true"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="$$$"
                    android:id="@+id/others_taxes_Val_total"
                    android:layout_column="5"

                    style="@style/text_line_val"
                    android:layout_below="@+id/othersTaxes"
                    android:layout_alignParentRight="true"/>




            </RelativeLayout>



        </RelativeLayout>



    </LinearLayout>
    </ScrollView>
</RelativeLayout>
