                    android:layout_height="wrap_content"
                    android:text="@string/tax_wage"
                    android:id="@+id/taxableWageTab"
                    android:layout_column="0"
                    style="@style/text_line_val_subTitle"
                    android:background="@drawable/line1_bg"
                    android:gravity="center_vertical|center_horizontal"/>


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="$$$"
                    android:id="@+id/taxableFreqVal"
                    android:layout_column="0"
                    style="@style/text_line_val"
                    android:layout_below="@+id/taxableWageTab"
                    android:layout_alignParentLeft="true"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="$$$"
                    android:id="@+id/TaxableInYearVal"
                    android:layout_column="5"

                    style="@style/text_line_val"
                    android:layout_below="@+id/taxableWageTab"
                    android:layout_alignParentRight="true"/>




            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/lyt_tax"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lyt_taxWage">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/taxes"
                    android:id="@+id/TaxeTab"
                    android:layout_column="0"
                    style="@style/text_line_val_subTitle"
                    android:background="@drawable/line1_bg"
                    android:gravity="center_vertical|center_horizontal"/>


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="$$$"
                    android:id="@+id/taxeFreqVal"
                    android:layout_column="0"
                    style="@style/text_line_val"
                    android:layout_below="@+id/TaxeTab"
                    android:layout_alignParentLeft="true"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="$$$"
                    android:id="@+id/taxeInYear"
                    android:layout_column="5"

                    style="@style/text_line_val"
                    android:layout_below="@+id/TaxeTab"
                    android:layout_alignParentRight="true"/>




            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/lyt_net"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lyt_tax">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/net_pay_label"
                    android:id="@+id/netPayTab"
                    android:layout_column="0"
                    style="@style/text_line_val_subTitle"
                    android:background="@drawable/line1_bg"
                    android:gravity="center_vertical|center_horizontal"/>


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="$$$"
                    android:id="@+id/netPayFreqVal"
                    android:layout_column="0"
