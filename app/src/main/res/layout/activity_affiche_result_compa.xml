                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="$$$"
                    android:id="@+id/ot_pay_total"
                    android:layout_column="5"

                    style="@style/text_line_val"
                    android:layout_below="@+id/ot_pay_title"
                    android:layout_alignParentRight="true"/>



            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/lyt_other_pay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lyt_ot_pay">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/other_benefits"
                    android:id="@+id/other_pay_title"
                    android:layout_column="0"
                    style="@style/text_line_val"
                    android:background="@drawable/line1_bg"
                    android:gravity="center_vertical|center_horizontal"/>


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="$$$"
                    android:id="@+id/other_pay_freq"
                    android:layout_column="0"
                    style="@style/text_line_val"
                    android:layout_below="@+id/other_pay_title"
                    android:layout_alignParentLeft="true"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="$$$"
                    android:id="@+id/other_pay_total"
                    android:layout_column="5"

                    style="@style/text_line_val"
                    android:layout_below="@+id/other_pay_title"
                    android:layout_alignParentRight="true"/>



            </RelativeLayout>



        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/lyt_content_pub2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:layout_marginBottom="0dp"
            android:layout_marginRight="0dp"
            android:layout_marginEnd="0dp"
            android:layout_below="@+id/tabGain"

            tools:ignore="RtlCompat"


            android:layout_alignParentTop="true">

            <com.google.android.gms.ads.AdView
                android:id="@+id/adView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                ads:adUnitId="@string/banner_ad_unit_id"
                ads:adSize="SMART_BANNER"

                />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:id="@+id/tabDeduction"
            android:layout_below="@+id/lyt_content_pub2"
            android:contentDescription="Pay Summary"
            android:background="@drawable/customshape"
            android:visibility="visible"
            android:layout_margin="@dimen/activity_horizontal_margin">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/deductions"
                android:id="@+id/DeductionTitle"
                android:textSize="@dimen/sub_title"
                android:layout_column="0"
                style="@style/section_title"
                android:layout_alignParentTop="true"/>


            <RelativeLayout
                android:id="@+id/lyt_deduc_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/DeductionTitle">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Frequency(weekly)"

                    android:id="@+id/deduc_freg"
                    android:layout_alignParentLeft="true"
                    style="@style/text_line_val"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:text="Total per Year"
                    android:id="@+id/deduc_total"
                    android:layout_alignParentRight="true"
                    style="@style/text_line_val"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/lyt_deduc_stand"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lyt_deduc_title">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/standard_deduction"
                    android:id="@+id/standardDeduction"
                    android:layout_column="0"
                    style="@style/text_line_val"
                    android:background="@drawable/line1_bg"
                    android:gravity="center_vertical|center_horizontal"/>


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Freq value"
                    android:id="@+id/standard_deduction_freg"
                    android:layout_column="0"
                    style="@style/text_line_val"
                    android:layout_below="@+id/standardDeduction"
                    android:layout_alignParentLeft="true"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Gross in year"
                    android:id="@+id/standard_deduction_total"
                    android:layout_column="5"

                    style="@style/text_line_val"
                    android:layout_below="@+id/standardDeduction"
                    android:layout_alignParentRight="true"/>

            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/lyt_deduc_perso"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lyt_deduc_stand">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/personal_deduction"
                    android:id="@+id/PersonnalDeduc"
                    android:layout_column="0"
                    style="@style/text_line_val"
                    android:background="@drawable/line1_bg"
                    android:gravity="center_vertical|center_horizontal"/>


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="$$$"
                    android:id="@+id/Personnal_deduction_freq"
                    android:layout_column="0"
                    style="@style/text_line_val"
                    android:layout_below="@+id/PersonnalDeduc"
                    android:layout_alignParentLeft="true"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="$$$"
                    android:id="@+id/Personnal_deduction_total"
                    android:layout_column="5"

                    style="@style/text_line_val"
                    android:layout_below="@+id/PersonnalDeduc"
                    android:layout_alignParentRight="true"/>



            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/lyt_deduc_other"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/lyt_deduc_perso">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/other_deduction"
                    android:id="@+id/otherDeduc"
                    android:layout_column="0"
                    style="@style/text_line_val"
                    android:background="@drawable/line1_bg"
                    android:gravity="center_vertical|center_horizontal"/>


                <TextView
                    android:layout_width="wrap_content"
